test "la capacidad de La Trastienda para el 05 de agosto del 2017 es de 700 por ser sabado" {
	assert.equals(700,(new Presentacion(new Date(05, 08, 2017), trastienda, #{luisAlberto}).cantidadPersonas()))
}

test "la capacidad de La Trastienda para el 08 de agosto del 2017 es de 400 por no ser sabado" {
	assert.equals(700,(new Presentacion(new Date(08, 08, 2017), trastienda, #{luisAlberto}).cantidadPersonas()))
}

test "presentacion con todos los artistas en lunapark cuesta 1550"{
	assert.equals(1550,( new Presentacion(new Date(20, 04, 2017), lunaPark, #{joaquin, lucia, luisAlberto}) ).costo())
}

test "presentacion con todos los artistas en trastienda cuesta 1650"{
	assert.equals(1650,( new Presentacion(new Date(15, 11, 2017), trastienda, #{joaquin, lucia, luisAlberto}) ).costo())
}